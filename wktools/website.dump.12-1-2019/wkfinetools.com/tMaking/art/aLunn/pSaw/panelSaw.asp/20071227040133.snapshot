					<tr>
						<td align="left" valign="bottom" width="488" height="26">
						<h4 style="margin-bottom: 0px">
				Making Fine Woodworking Tools</h4>
						</td>
						<td width="10" height="26" valign="bottom">&nbsp;</td>
						<td align="center" width="100" height="26" valign="bottom">
							&nbsp;</td>
					</tr>
