					<tr>
						<td height="23">
						<p class="subtitle">Drop Forging
						<span style="font-weight: 400"><font size="2">by 
						Machinery's Reference Series, 1910</font></span></td>
						<td width="10" height="23">&nbsp;</td>
						<td align="right" width="130" height="23">
						<font SIZE="2">
						<a href="../tMaking-index-Reading.asp">
						<img border="0" src="../../../0_xGlobal-incl/0_img-Nav/arrowBack.jpg" width="24" height="24"></a></font></td>
					</tr>
